{
    "endOfLine": "auto",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "printWidth": 90,
    "arrowParens": "always",
    "bracketSameLine": false,
    "importOrder": [
        "^react/(.*)$",
        "^(redux/(.*)$)",
        "^(pages/(.*)$)",
        "^(components/(.*)$)",
        "^(utils/(.*)$)",
        "^(hooks/(.*)$)",
        "^(ts/(.*)$)",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
