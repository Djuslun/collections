{
    "endOfLine": "auto",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "printWidth": 90,
    "arrowParens": "always",
    "bracketSameLine": false,
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": [
        "^react/(.*)$",
        "^(redux/(.*)$)",
        "^(pages/(.*)$)",
        "^(components/(.*)$)",
        "^(utils/(.*)$)",
        "^(hooks/(.*)$)",
        "^(ts/(.*)$)",
        "^(ui/(.*)$)",
        "^(languages/(.*)$)",
        "^[./]"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
